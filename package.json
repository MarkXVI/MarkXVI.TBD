{
  "name": "markxvi-webpage",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run server\" \"npm run go\"",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-start\"",
